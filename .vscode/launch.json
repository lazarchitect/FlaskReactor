	"version": "0.2.0",
	"configurations": [
		{
			"name": "FlaskReactor LOCAL DB",
			"type": "debugpy",
			"request": "launch",
			"module": "src.app",
			"sudo": true, //could cause issues on windows?
			"args": [
				"local_db"
			],
			"preLaunchTask": "webpack build"
		},
		{
			"name": "FlaskReactor REMOTE DB",
			"type": "debugpy",
			"request": "launch",
			"module": "src.app",
			"sudo": true, //could cause issues on windows?
			"args": [
				"remote_db"
			],
			"preLaunchTask": "webpack build"
		},
		{
			"name": "FlaskReactor NO DB",
			"type": "debugpy",
			"request": "launch",
			"module": "src.app",
			"sudo": true, //could cause issues on windows?
			"args": [
				"no_db"
			],
			"preLaunchTask": "webpack build"
		}
	]
}