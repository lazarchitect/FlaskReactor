	"version": "0.2.0",
	"configurations": [
		{
			"name": "FlaskReactor LOCAL DB",
			"type": "debugpy",
			"request": "launch",
			"module": "src.app",
			"env": {"db_env": "local"},
			// "args": [
			// 	"local_db"
			// ],

			"sudo": true, //could cause issues on windows?
			"preLaunchTask": "webpack build"
		},
		{
			"name": "FlaskReactor REMOTE DB",
			"type": "debugpy",
			"request": "launch",
			"module": "src.app",
			"env": {"db_env": "remote"},
			// "args": [
			// 	"remote_db"
			// ],
			"sudo": true, //could cause issues on windows?
			"preLaunchTask": "webpack build"
		},
		{
			"name": "FlaskReactor NO DB",
			"type": "debugpy",
			"request": "launch",
			"module": "src.app",
			"env": {"db_env": "none"},
			// "args": [
			// 	"no_db"
			// ],
			"sudo": true, //could cause issues on windows?
			"preLaunchTask": "webpack build"
		}
	]
}